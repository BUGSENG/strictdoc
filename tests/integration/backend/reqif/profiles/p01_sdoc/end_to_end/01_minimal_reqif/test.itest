RUN: %strictdoc export --formats=reqif-sdoc %S/sample.sdoc
RUN: %strictdoc import reqif sdoc %S/output/reqif/output.reqif %S/output/sample.sdoc
RUN: %diff %S/sample.sdoc %S/output/sample.sdoc

RUN: %cat %S/output/reqif/output.reqif | filecheck %s
CHECK: <REQ-IF xmlns="http://www.omg.org/spec/ReqIF/20110401/reqif.xsd" xmlns:xhtml="http://www.w3.org/1999/xhtml">

RUN: %strictdoc export --formats=reqif-sdoc --reqif-profile=p02_simple --reqif-enable-mid %S/sample.sdoc
RUN: %strictdoc import reqif p02_simple %S/output/reqif/output.reqif %S/output/sample.sdoc
RUN: %diff %S/sample.sdoc %S/output/sample.sdoc
