<div class="main">
  <div class="dashboard">
    <div class="dashboard-main">
      {% include "screens/project_index/frame_project_tree.jinja.html" %}
    </div>
    <div class="dashboard-aside">
      <div class="dashboard-block" id="project_tree_controls">{# must to be empty #}</div>
      <div class="dashboard-block">
        {# {{ view_object.project_config.project_title }} #}
        {# ⛭ #}
        <div class="dashboard-block-title">
          ⚙️&nbsp;&nbsp;Project tree configuartion
        </div>

        <b>Document paths:</b>
          <ul>
            {% for path_ in view_object.project_config.include_doc_paths %}
            <li style="list-style-type: '✔️    '">{{ path_ }}</li>
            {% endfor %}
            {% for path_ in view_object.project_config.exclude_doc_paths %}
            <li style="list-style-type: '⛔    '">{{ path_ }}</li>
            {% endfor %}

          </ul>

        <b>Source path:</b>
        <div>
          <code style="word-wrap: break-word;">{{ view_object.project_config.source_root_path }}</code>
        </div>
          <ul>
            {% for path_ in view_object.project_config.include_source_paths %}
            <li style="list-style-type: '✔️    '">{{ path_ }}</li>
            {% endfor %}
            {% for path_ in view_object.project_config.exclude_source_paths %}
            <li style="list-style-type: '⛔    '">{{ path_ }}</li>
            {% endfor %}
          </ul>

      </div>
    </div>
  </div>
</div>
